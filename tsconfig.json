{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components": ["src/core/components"],
      "@components/*": ["src/core/components/*"],
      "@interfaces": ["src/core/interfaces"],
      "@interfaces/*": ["src/core/interfaces/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"],
      "@utils": ["src/core/utils"],
      "@utils/*": ["src/core/utils/*"],
      "@store/*": ["src/store/*"],
      "@guards/*": ["src/guards/*"],
      "@hooks": ["src/store/hooks"],
      "@hooks/*": ["src/store/hooks/*"],
      "@screens": ["src/screens"],
      "@screens/*": ["src/screens/*"]
    }
  }
}
